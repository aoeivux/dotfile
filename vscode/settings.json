				"<space>"
			],
			"commands": [
				"whichkey.show"
			]
		}
	],
	"vim.normalModeKeyBindingsNonRecursive": [
		{
			"before": [
				"S"
			],
			"commands": [
				"workbench.action.files.save"
			]
		},
		{
			"before": [
				"Q"
			],
			"commands": [
				":q"
			]
		},
		{
			"before": [
				"H"
			],
			"after": [
				"^"
			]
		},
		{
			"before": [
				"L"
			],
			"after": [
				"$"
			]
		},
		{
			"before": [
				"<leader>",
				"l"
			],
			"commands": [
				":nohl"
			]
		},
		{
			"before": [
				";",
				"f",
				"m"
			],
			"commands": [
				"editor.action.formatDocument"
			]
		},
		{
			"before": [
				"K"
			],
			"after": [
				"3",
				"k"
			],
			"silent": true
		},
		{
			"before": [
				"J"
			],
			"after": [
				"3",
				"j"
			],
			"silent": true
		},
		{
			"before": [
				"<leader>",
				"u"
			],
			"commands": [
				"editor.action.referenceSearch.trigger"
			]
		},
		{
			"before": [
				"<leader>",
				"U"
			],
			"commands": [
				"references-view.findReferences"
			]
		},
		{
			"before": [
				"<leader>",
				"i",
				"n"
			],
			"commands": [
				"references-view.showCallHierarchy"
			]
		},
		{
			"before": [
				"<leader>",
				"r"
			],
			"commands": [
				"workbench.action.files.save",
				"workbench.action.debug.run"
			]
		},
		{
			"before": [
				"<leader>",
				"t"
			],
			"commands": [
				"testing.debugLastRun"
			]
		},
		{
			"before": [
				"<leader>",
				"d",
				"b"
			],
			"commands": [
				"editor.debug.action.toggleBreakpoint"
			]
		},
		{
			"before": [
				"<leader>",
				"d",
				"c"
			],
			"commands": [
				"editor.debug.action.conditionalBreakpoint"
			]
		},
		{
			"before": [
				"[",
				"e"
			],
			"commands": [
				"editor.action.marker.prev"
			]
		},
		{
			"before": [
				"]",
				"e"
			],
			"commands": [
				"editor.action.marker.next"
			]
		},
		{
			"before": [
				"<leader>",
				"h",
				"k"
			],
			"commands": [
				"workbench.action.editor.previousChange",
				"editor.action.dirtydiff.previous"
			]
		},
		{
			"before": [
				"<leader>",
				"h",
				"j"
			],
			"commands": [
				"workbench.action.editor.nextChange",
				"editor.action.dirtydiff.next"
			]
		},
		{
			"before": [
				"<leader>",
				"h",
				"s"
			],
			"commands": [
				"git.stage"
			]
		},
		{
			"before": [
				"<leader>",
				"h",
				"u"
			],
			"commands": [
				"git.unstage"
			]
		},
		{
			"before": [
				"<leader>",
				"h",
				"r"
			],
			"commands": [
				"git.revertSelecFtedRanges"
			]
		},
		{
			"before": [
				"<leader>",
				"r",
				"n"
			],
			"commands": [
				"editor.action.rename"
			]
		},
		{
			"before": [
				"<leader>",
				"c",
				"a"
			],
			"commands": [
				"editor.action.quickFix"
			]
		},
		{
			"before": [
				"<leader>",
				"c",
				"A"
			],
			"commands": [
				"keyboard-quickfix.openQuickFix"
			]
		}
	],
	"vim.visualModeKeyBindings": [
		{
			// which key
			"before": [
				"<space>"
			],
			"commands": [
				"whichkey.show"
			]
		},
		// repeatly indent in visual mode
		{
			"before": [
				">"
			],
			"commands": [
				"editor.action.indentLines"
			]
		},
		{
			"before": [
				"<"
			],
			"commands": [
				"editor.action.outdentLines"
			]
		}
	],
	"vim.visualModeKeyBindingsNonRecursive": [
		{
			"before": [
				"<leader>",
				"f"
			],
			"commands": [
				"editor.action.formatSelection"
			]
		},
		{
			"before": [
				"K"
			],
			"after": [
				"3",
				"k"
			],
			"silent": true
		},
		{
			"before": [
				"J"
			],
			"after": [
				"3",
				"j"
			]
		},
		{
			// p action will not override clipboard content
			"before": [
				"p"
			],
			"after": [
				"p",
				"g",
				"v",
				"y"
			]
		},
		{
			"before": [
				"H"
			],
			"after": [
				"^"
			]
		},
		{
			"before": [
				"L"
			],
			"after": [
				"$"
			]
		}
	],
	/* vim settings end */
	/* which key bindings */
	"whichkey.bindings": [
		{
			"key": "e",
			"name": "Show tree/explorer view",
			"type": "conditional",
			"bindings": [
				{
					"key": "",
					"name": "default",
					"type": "command",
					"command": "workbench.view.explorer"
				},
				{
					"key": "when:sideBarVisible && explorerViewletVisible",
					"name": "Hide explorer",
					"type": "command",
					"command": "workbench.action.toggleSidebarVisibility"
				}
			]
		},
		{
			"key": "g",
			"name": "GitRepo",
			"type": "command",
			"command": "workbench.view.scm"
		},
		{
			"key": "G",
			"name": "Goto Marketplace",
			"type": "command",
			"command": "workbench.view.extensions"
		},
		{
			"command": "workbench.view.search",
			"key": "w",
			"name": "Search Word",
			"type": "command"
		},
		{
			"key": "b",
			"name": "Buffer Manage...",
			"type": "bindings",
			"bindings": [
				{
					"key": "o",
					"name": "Close other buffers",
					"type": "command",
					"command": "workbench.action.closeOtherEditors"
				},
				{
					"key": "a",
					"name": "Close all buffers",
					"type": "command",
					"command": "workbench.action.closeEditorsInGroup"
				}
			]
		},
		{
			"key": "f",
			"name": "File...",
			"type": "bindings",
			"bindings": [
				{
					"key": "f",
					"name": "New Search Editor",
					"type": "command",
					"command": "search.action.openEditor"
				},
				{
					"command": "workbench.action.quickOpen",
					"key": "r",
					"name": "Recent Files",
					"type": "command"
				}
			]
		},
		{
			"command": "workbench.action.showAllSymbols",
			"key": "S",
			"name": "Search project symbol",
			"type": "command"
		},
		{
			"command": "workbench.actions.view.problems",
			"key": "q",
			"name": "Show All Problems",
			"type": "command"
		},
		{
			"command": "workbench.action.toggleAuxiliaryBar",
			"key": "o",
			"name": "Outline",
			"type": "command"
		},
		{
			"command": "javaProjectExplorer.focus",
			"key": "j",
			"name": "Java Project",
			"type": "command"
		},
		{
			"command": "github.cweijan.mysql.focus",
			"key": "m",
			"name": "Sql Manager Tool Focus",
			"type": "command"
		}
	],
